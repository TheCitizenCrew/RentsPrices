{
    "name": "Prix des Loyers",
    "description": "Crowdsourcing des prix des loyers",
    "keywords": ["crowdsourcing", "opendata", "transparence"],
    "license": "GPL v3",
    "type": "project",
    "require": {
				"laravel/lumen-framework": "5.0.*",
				"doctrine/dbal": "2.5.1",
				"vlucas/phpdotenv": "~1.0",
				"aziz/smartmodel": "2.*",
				"venturecraft/revisionable": "dev-master",
				"phpoffice/phpexcel" : "1.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
